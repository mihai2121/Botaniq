        return fertilityRequirement;
    }

    public void setFertilityRequirement(String fertilityRequirement) {
        this.fertilityRequirement = fertilityRequirement;
    }

    public String getPhMin() {
        return phMin;
    }

    public void setPhMin(String phMin) {
        this.phMin = phMin;
    }

    public String getPhMax() {
        return phMax;
    }

    public void setPhMax(String phMax) {
        this.phMax = phMax;
    }

    public String getBloomPeriod() {
        return bloomPeriod;
    }

    public void setBloomPeriod(String bloomPeriod) {
        this.bloomPeriod = bloomPeriod;
