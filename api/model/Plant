package com.wade.botaniq.models;

import javax.xml.bind.annotation.XmlRootElement;

/**
 *
 * @author psg
 */
@XmlRootElement
public class Plant {

    private String abrev;
    private String abstractd;
    private String linkWiki;
    private String scientificName;
    private String commonName;
    private String genus;
    private String family;
    private String order;
    private String classd;
    private String division;
    private String duration;
    private String growthHabit;
    private String factSheet;
    private String plantGuide;
    private String flowerColor;
    private String growthRate;
    private String heigthMax;
    private String leafRetention;
    private String lifespan;
    private String toxicity;
    private String soilCoarse;
    private String soilMedium;
    private String soilFine;
    private String drougthTolerance;
    private String fertilityRequirement;
    private String phMin;
    private String phMax;
    private String shadeTolerance;
    private String bloomPeriod;
    private String fruitPeriod;    
    private String propogateByRoot;
    private String propogateByBulbs;
    private String propogateByContainer;
    private String propogateByCorms;
    private String propogateByCutting;
    private String propogateBySeed;
    private String propogateBySood;
    private String propogateBySprings;
    private String propogateByTubers;
    private String image;

    public Plant() {
    }

    public Plant(String abrev, String abstractd, String linkWiki, String scientificName, String commonName, String genus, String family, String order, String classd, String division, String duration, String growthHabit, String factSheet, String plantGuide, String flowerColor, String growthRate, String heigthMax, String leafRetention, String lifespan, String toxicity, String soilCoarse, String soilMedium, String soilFine, String drougthTolerance, String fertilityRequirement, String phMin, String phMax, String shadeTolerance, String bloomPeriod, String fruitPeriod,String propogateByRoot, String propogateByBulbs, String propogateByContainer, String propogateByCorms, String propogateByCutting, String propogateBySeed, String propogateBySood, String propogateBySprings, String propogateByTubers, String image) {
        this.abrev = abrev;
        this.abstractd = abstractd;
        this.linkWiki = linkWiki;
        this.scientificName = scientificName;
        this.commonName = commonName;
        this.genus = genus;
        this.family = family;
        this.order = order;
        this.classd = classd;
        this.division = division;
        this.duration = duration;
        this.growthHabit = growthHabit;
        this.factSheet = factSheet;
        this.plantGuide = plantGuide;
        this.flowerColor = flowerColor;
        this.growthRate = growthRate;
        this.heigthMax = heigthMax;
        this.leafRetention = leafRetention;
        this.lifespan = lifespan;
        this.toxicity = toxicity;
        this.soilCoarse = soilCoarse;
        this.soilMedium = soilMedium;
        this.soilFine = soilFine;
        this.drougthTolerance = drougthTolerance;
        this.fertilityRequirement = fertilityRequirement;
        this.phMin = phMin;
        this.phMax = phMax;
        this.shadeTolerance = shadeTolerance;
        this.bloomPeriod = bloomPeriod;
        this.fruitPeriod=fruitPeriod;
        this.propogateByRoot = propogateByRoot;
        this.propogateByBulbs = propogateByBulbs;
        this.propogateByContainer = propogateByContainer;
        this.propogateByCorms = propogateByCorms;
        this.propogateByCutting = propogateByCutting;
        this.propogateBySeed = propogateBySeed;
        this.propogateBySood = propogateBySood;
        this.propogateBySprings = propogateBySprings;
        this.propogateByTubers = propogateByTubers;
        this.image = image;
    }

